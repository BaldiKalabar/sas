                    if rp == 4:
                        pole4[cordi1][cordi2] = 'üü´'
                        cordi2 = cordi2 - 1
                        if pole4[cordi1][cordi2] == 'üß±':
                            pole4[cordi1][cordi2] = 'üß±'
                            cordi1 = 2
                            cordi2 = 1
                            pole4[cordi1][cordi2] = skin_pers
                        if pole4[cordi1][cordi2] == 'üíé':
                            Endwin = Endwin + 1
                            mapgame = 0
                        pole4[cordi1][cordi2] = skin_pers
                if rg == ProtScet:
                    mapgame = 0
                    fight = 1
        while fight == 1:
            # –ò–≥—Ä–∞ –Ω–∞ –±–∏—Ç–≤–µ
            if slosnot == 1:
                ENEMY['Health'] = random.randint(400, 450)
                ENEMY['Hand'] = random.randint(50, 70)
                CHRS['–°–∏–ª–∞'] = random.randint(100, 150)
                CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] = random.randint(10, 20)
