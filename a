                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(4, 7)
                if slosnot == 4:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(3, 5)
            if CHRS['–°–∏–ª–∞'] > 35 and CHRS['–°–∏–ª–∞'] < 245:
                if slosnot == 1:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(8, 10)
                if slosnot == 2:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(6, 8)
                if slosnot == 3:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(4, 7)
                if slosnot == 4:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(3, 5)
            for iter in range(max):
                temp = int(round(iter * 100 / 1000) // 2)
                if count >= length_load - 1:
                    count = 0
                else:
                    count += 1
                    time.sleep(0.1)
                print(heiqght * '\n', f'{symb_load[count]} - –ó–∞–≥—Ä—É–∑–∫–∞', heiqght * '\n')
            max = random.randint(10, 50)
            symb_load = '\\|/-\\|/-'
            length_load = len(symb_load)
            count = 0
            heiqght = 10
            for iter in range(max):
                temp = int(round(iter * 100 / 1000) // 2)
                if count >= length_load - 1:
                    count = 0
                else:
