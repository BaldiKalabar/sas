            print("–í—ã–Ω–æ—Å–ª–∏–≤–æ—Å—Ç—å", CHRS["–í—ã–Ω–æ—Å–ª–∏–≤–æ—Å—Ç—å"])
            print("–°–∏–ª–∞", CHRS["–°–∏–ª–∞"])
            print("–∑–µ–ª—å–∫–∏", inventory["–ó–µ–ª—å–µ –∑–¥–æ—Ä–æ–≤—å—è"])
            health_icon = '‚ù§'
