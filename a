                time.sleep(4)
            if ENEMY["Health"] < 0:
                if run == 1:
                    if slosnot == 1:
                        ProtScet = random.randint(5, 8)
                    if slosnot == 2:
                        ProtScet = random.randint(4, 6)
                    if slosnot == 3:
                        ProtScet = random.randint(3, 5)
                    if slosnot == 4:
                        ProtScet = random.randint(2, 4)
                    fight = 0
                    mapgame = 1
                    time.sleep(4)
                    print("–í–æ–∑—Ä–∞—à–∞–µ–º—Å—è –Ω–∞ –∫–∞—Ä—Ç—É...")
                    time.sleep(3)
                    rg = 0
                else:
                    print("–ü—Ä–æ—Ç–∏–≤–Ω–∏–∫ —É–º–µ—Ä, –ü–û–ë–ï–î–êüéâüéâüéâ")
                    if slosnot == 1:
                        ProtScet = random.randint(5, 8)
                    if slosnot == 2:
                        ProtScet = random.randint(4, 6)
                    if slosnot == 3:
                        ProtScet = random.randint(3, 5)
                    if slosnot == 4:
                        ProtScet = random.randint(2, 4)
                    fight = 0
                    mapgame = 1
                    time.sleep(4)
                    print("–í–æ–∑—Ä–∞—à–∞–µ–º—Å—è –Ω–∞ –∫–∞—Ä—Ç—É...")
                    time.sleep(3)
                    rg = 0
    if smerti == 1:
        loop = input("–¢—ã —É–º–µ—Ä... –¢—ã —ç—Ç–æ –∑–Ω–∞–µ—à? –¢–∞–∫ –≤–æ—Ç, —Ö–æ—á–µ—à—å –∑–∞–Ω–æ–≥–æ –Ω–∞—á–∞—Ç—å? \n (–î–ê) --- (–ù–ï–¢)")
        win=0
        while loop != "–î–ê" and loop != '–î–∞' and loop != '–¥–∞' and loop != '–Ω–µ—Ç' and loop != '–ù–ï–¢' and loop != '–ù–µ—Ç':
            loop = input("–¢—ã —É–º–µ—Ä... –¢—ã —ç—Ç–æ –∑–Ω–∞–µ—à? –¢–∞–∫ –≤–æ—Ç, —Ö–æ—á–µ—à—å –∑–∞–Ω–æ–≥–æ –Ω–∞—á–∞—Ç—å? \n (–î–ê) --- (–ù–ï–¢)")
            win=0
    if win == 1:
        loop = input("–¢—ã –ø–æ–±–µ–¥–∏–ª, –Ω–æ –≥–æ—Ç–æ–≤ —Ç—ã –Ω–∞—á–∞—Ç—å –µ—â—ë  —Ä–∞–∑? \n (–î–ê) --- (–ù–ï–¢)")
        while loop != "–î–ê" and loop != '–î–∞' and loop != '–¥–∞' and loop != '–Ω–µ—Ç' and loop != '–ù–ï–¢' and loop != '–ù–µ—Ç':
            loop = input("–¢—ã –ø–æ–±–µ–¥–∏–ª, –Ω–æ –≥–æ—Ç–æ–≤ —Ç—ã –Ω–∞—á–∞—Ç—å –µ—â—ë  —Ä–∞–∑? \n (–î–ê) --- (–ù–ï–¢)")
    if loop == "–î–ê":
        loop = 1
        Endwin = 0
        mapgame = 1
        smerti = 0
        win = 0
        rg = 0
    if loop == '–¥–∞':
        loop = 1
        Endwin = 0
        mapgame = 1
        smerti = 0
        win = 0
        rg = 0
    if loop == '–î–∞':
        loop = 1
        Endwin = 0
        mapgame = 1
        smerti = 0
        win = 0
        rg= 0
    if loop == "–ù–ï–¢":
        loop = 0
        mapgame = 0
        smerti = 0
        win = 0
        Endwin = 0
    if loop == '–Ω–µ—Ç':
        loop = 0
        mapgame = 0
        smerti = 0
        win = 0
        Endwin = 0
    if loop == '–ù–µ—Ç':
        loop = 0
        mapgame = 0
        smerti = 0
        win = 0
        Endwin = 0
