            if CHRS['–°–∏–ª–∞'] > 10 and CHRS['–°–∏–ª–∞'] < 25:
                if slosnot == 1:
                    inventory['–ó–µ–ª—å—è –∑–¥–æ—Ä–æ–≤—å—è'] = random.randint(6, 10)
