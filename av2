            print("–∑–µ–ª—å–∫–∏", inventory["–ó–µ–ª—å–µ –∑–¥–æ—Ä–æ–≤—å—è"])
            health_icon = '‚ù§'
            Hero_health_bar = int(HERO['3'] // 10) * health_icon
            Enemy_bar_health_icon = '‚ù§'
            Enemy_bar_health_bar = ENEMY['Health'] * Enemy_bar_health_icon
            print(
                f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name} - {Hero_health_bar}  \n------> –ó–¥–æ—Ä–æ–≤—å–µ {NAME} \n{NAME} - {Enemy_bar_health_bar}')
            while HERO["3"] > 0 and ENEMY['Health'] > 0:
                print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –∞—Ç–∞–∫", CHRS["–£—Å—Ç–∞–ª–æ—Å—Ç—å"])
                if NAME == '–ë–æ—Å—Å':
                    d = input(
                        "-----> ‚öîÔ∏è–ù–∞–∂–º–∏—Ç–µ –¥–ª—è –∞—Ç–∞–∫–∏ (a-—Ñ)\n-----> üß™–î–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∑–µ–ª—å–∫–∏ (s-—ã) \n-----> üî•–ß—Ç–æ –±—ã –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Å–∏–ª—ã (d-–≤) \n-----> üèÉ‚Äç‚û°Ô∏è–ß—Ç–æ –±—ã —Å–±–µ–∂–∞—Ç—å ERROR ")
                    print(heiqght * '\n', f'{symb_load[count]} - –ó–∞–≥—Ä—É–∑–∫–∞', heiqght * '\n')
                    while d != "a" and d != "s" and d != "d" and d != "f" and d != "—Ñ" and d != "—ã" and d != "–≤" and d != "–∞":
                        d = input(
                            "-----> ‚öîÔ∏è–ù–∞–∂–º–∏—Ç–µ –¥–ª—è –∞—Ç–∞–∫–∏ (a-—Ñ)\n-----> üß™–î–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∑–µ–ª—å–∫–∏ (s-—ã) \n-----> üî•–ß—Ç–æ –±—ã –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Å–∏–ª—ã (d-–≤) \n-----> üèÉ‚Äç‚û°Ô∏è–ß—Ç–æ –±—ã —Å–±–µ–∂–∞—Ç—å ERROR ")
                        print(heiqght * '\n', f'{symb_load[count]} - –ó–∞–≥—Ä—É–∑–∫–∞', heiqght * '\n')
                else:
                    d = input(
                        "-----> ‚öîÔ∏è–ù–∞–∂–º–∏—Ç–µ –¥–ª—è –∞—Ç–∞–∫–∏ (a-—Ñ)\n-----> üß™–î–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∑–µ–ª—å–∫–∏ (s-—ã) \n-----> üî•–ß—Ç–æ –±—ã –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Å–∏–ª—ã (d-–≤) \n-----> üèÉ‚Äç‚û°Ô∏è–ß—Ç–æ –±—ã —Å–±–µ–∂–∞—Ç—å PRESS (F-–∞) ")
                    print(heiqght * '\n', f'{symb_load[count]} - –ó–∞–≥—Ä—É–∑–∫–∞', heiqght * '\n')
                    while d != "a" and d != "s" and d != "d" and d != "f" and d != "—Ñ" and d != "—ã" and d != "–≤" and d != "–∞":
                        d = input(
                            "-----> ‚öîÔ∏è–ù–∞–∂–º–∏—Ç–µ –¥–ª—è –∞—Ç–∞–∫–∏ (a-—Ñ)\n-----> üß™–î–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∑–µ–ª—å–∫–∏ (s-—ã) \n-----> üî•–ß—Ç–æ –±—ã –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Å–∏–ª—ã (d-–≤) \n-----> üèÉ‚Äç‚û°Ô∏è–ß—Ç–æ –±—ã —Å–±–µ–∂–∞—Ç—å PRESS (F-–∞) ")
                        print(heiqght * '\n', f'{symb_load[count]} - –ó–∞–≥—Ä—É–∑–∫–∞', heiqght * '\n')
                if d == 'a' or d == '—Ñ':
                    if CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] != 0:
                        CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] -= 1
                        crit = random.randint(0, 20)
                        print('‚öîÔ∏è–ö—Ä–∏—Ç–∏—á–µ—Å–∫–∏–π —É—Ä–æ–Ω —Å–æ—Å—Ç–∞–≤–∏–ª ', crit)
                        ENEMY["Health"] = ENEMY["Health"] - HERO["–ë–∞–∑–æ–≤—ã–π —É—Ä–æ–Ω"] - crit - inventory['–º–µ—á']
                        Enemy_bar_health_bar = ENEMY['Health'] * Enemy_bar_health_icon
                        print(f'----->‚öîÔ∏è{NAME} –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω \n{NAME} - {Enemy_bar_health_bar}')
                        if random.randint(1, 10) in [1, 2, 3, 4, 5]:
                            critprotiv = random.randint(0, 20)
                            print(f"----->‚öîÔ∏è{name} –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω")
                            HERO["3"] = HERO["3"] - ENEMY["Hand"] + inventory["–ë—Ä–æ–Ω—è"] - critprotiv + inventory['—â–∏—Ç']
                            Hero_health_bar = int(HERO['3'] // 10) * health_icon
                            print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                        else:
                            print(f"-----> {name} –∑–∞—â–∏—Ç–∏–ª—Å—èüõ°Ô∏èüõ°Ô∏èüõ°Ô∏è")
                    else:
                        print("–í—ã —É—Å—Ç–∞–ª–∏, –≤–∞–º —Å—Ç–æ–∏—Ç –æ—Ç–¥–æ—Ö–Ω—É—Ç—å")
                if d == 'f' or d == '–∞':
                    if NAME == '–ë–æ—Å—Å':
                        print('ERROR')
                    else:
                        if CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] > 0:
                            if random.randint(1, 15) in [1]:
                                print("–í—ã —Å–±–µ–∂–∞–ª–∏")
                                run = 1
                                ENEMY["Health"] = -1000000
                            else:
                                CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] -= 1
                                HERO["3"] = HERO["3"] - ENEMY["Hand"] + inventory["–ë—Ä–æ–Ω—è"] - random.randint(30, 50) + \
                                            inventory['—â–∏—Ç'] + 10
                                print(f"‚öîÔ∏è{NAME} –Ω–µ –æ—Ç–ø—É—Å–∫–∞–µ—Ç –≤–∞—Å")
                                Hero_health_bar = int(HERO['3'] // 10) * health_icon
                                print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                        else:
                            print("–í—ã —É—Å—Ç–∞–ª–∏, –≤–∞–º —Å—Ç–æ–∏—Ç –æ—Ç–¥–æ—Ö–Ω—É—Ç—å")
                if d == 's' or d == '—ã':
                    if inventory["–ó–µ–ª—å–µ –∑–¥–æ—Ä–æ–≤—å—è"] > 0:
                        inventory['–ó–µ–ª—å–µ –∑–¥–æ—Ä–æ–≤—å—è'] -= 1
                        HERO["3"] += 100
                        print(f"-----> {name} –≤—ã–ª–µ—á–µ–ª—Å—è üß™")
                        Hero_health_bar = int(HERO['3'] // 10) * health_icon
                        print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                        if random.randint(1, 6) in [1, 2, 3, 6]:
                            HERO["3"] = HERO["3"] - ENEMY["Hand"] + inventory["–ë—Ä–æ–Ω—è"] + inventory['—â–∏—Ç'] + 10
                            print(f"‚öîÔ∏è{NAME} –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–æ–ª—Å—è –º–æ–º–µ–Ω—Ç–æ–º –∏ –Ω–∞–Ω–µ—Å —É—Ä–æ–Ω")
                            Hero_health_bar = int(HERO['3'] // 10) * health_icon
                            print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                    else:
                        print("-----> –ó–µ–ª—å–∫–∏ –±–æ–ª—å—à–µ –Ω–µ—Ç")
                        if random.randint(1, 6) in [1, 2, 3, 6]:
                            HERO["3"] = HERO["3"] - ENEMY["Hand"] + inventory["–ë—Ä–æ–Ω—è"] + 40
                            print("‚öîÔ∏è{NAME} –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–æ–ª—Å—è –º–æ–º–µ–Ω—Ç–æ–º –∏ –Ω–∞–Ω–µ—Å —É—Ä–æ–Ω")
                            Hero_health_bar = int(HERO['3'] // 10) * health_icon
                            print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                if d == 'd' or d == '–≤':
                    if random.randint(1, 10) in [1, 2, 3, 4, 5, 6, 7, 8]:
                        p = random.randint(1, 13)
                        if p in [1]:
                            print(HERO['name'], "–∞–≥—Ä–µ—Å–∏–≤–Ω–æ —Å–º–æ—Ç—Ä–∏—Ç –Ω–∞ ", ENEMY['NAME'])
                        if p in [2]:
                            print(HERO['name'], "–Ω–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞–µ—Ç –∫–∞–∫ –∏ ",ENEMY['NAME'])
                        if p in [3]:
                            print(HERO['name'], "–¥—É–º–∞–µ—Ç –æ –≤–µ–ª–∏–∫–æ–º –∏ –∑–∞–±—ã–≤–∞–µ—Ç –ø—Ä–æ –±–∏—Ç–≤—É, –∫–∞–∫ –∏ ",ENEMY['NAME'])
                        if p in [4]:
                            print(HERO['name'], "—Å–ø—Ä—è—Ç–∞–ª—Å—è, —Ç–µ—Ö–Ω–∏—á–µ—Å–∫–æ–µ –æ—Ç—Å—Ç—É–ø–ª–µ–Ω–∏–µ")
                        if p in [5]:
                            print("–ù–∏–∫—Ç–æ –Ω–µ –ø–æ–ø–∞–ª")
                        if p in [6]:
                            print("–í—ã –∏–∑—É—á–∞–µ—Ç–µ –¥—Ä—É–≥ –¥—Ä—É–≥–∞ –¥–ª—è —Ç–æ–≥–æ —á—Ç–æ –±—ã –Ω–∞–Ω–µ—Å—Ç–∏ –±–æ–ª–µ–µ —Å–∏–ª—å–Ω—ã–π —É–¥–∞—Ä")
                        if p in [7]:
                            print(f"–í—ã –ø—ã—Ç–∞–µ—Ç–µ—Å—å —É–∫—Ä–∞—Ç—å –∑–µ–ª—å—è {NAME} –Ω–æ —É –≤–∞—Å —ç—Ç–æ –Ω–µ –ø–æ–ª—É—á–∞–µ—Ç—Å—è")
                        if p in [8]:
                            print("–í—ã —É—Å—Ç–∞–ª–∏ –Ω–æ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç–µ –±–æ—Ä–æ—Ç—å—Å—è")
                        if p in [9]:
                            print("–í—ã –ø—Ä—è—á–∏—Ç–µ—Å—å —á—Ç–æ –±—ã –æ—Ç–¥–æ—Ö–Ω—É—Ç—å –æ—Ç —ç—Ç–æ–π –±–∏—Ç–≤—ã , —Ç–µ–ø–µ—Ä—å –≤—ã –º–æ–∂–µ—Ç–µ –Ω–∞–Ω–µ—Å—Ç–∏ —É–¥–∞—Ä")
                        if p in [10]:
                            print(
                                f"–í—ã –æ—Ç–≤–ª–µ–∫–∞–µ—Ç–µ {NAME} –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ –¥–æ–ª–≥–æ —á—Ç–æ –±—ã –≤–æ—Å—Ç–æ–Ω–æ–≤–∏—Ç—å —Å–∏–ª—ã –¥–ª—è —Å–ª–µ–¥—É—â–µ–≥–æ —É–¥–∞—Ä–∞")
                        if p in [11]:
                            print("–í—ã –≤—ã–ø—É—Å—Ç–∏–ª–∏ –ø–∞—É–∫–æ–≤, –æ—Ç–∫—É–¥–∞ –æ–Ω–∏ —É –≤–∞—Å? –ù–µ–≤–∞–∂–Ω–æ –≤—ã –º–æ–∂–µ—Ç–µ –∞—Ç–∞–∫–æ–≤–∞—Ç—å ")
                        if p in [12]:
                            print(HERO["name"], "–ù–µ —Å–¥–∞–≤–∞–π—Å—è, –≤ —Ç–µ–±—è –∏ —Ç–∞–∫ –Ω–∏–∫—Ç–æ –Ω–µ –≤–µ—Ä–∏—Ç")
                        if p in [13]:
                            print(HERO["name"], "–ù–∞–ø–æ–ª–Ω—è–µ—Ç—Å—è —Ä–µ—à–∏–º–æ—Å—Ç—å—é –¥–æ–≤–µ—Å—Ç–∏ –±–∏—Ç–≤—É –¥–æ –∫–æ–Ω—Ü–∞")
                        CHRS['–£—Å—Ç–∞–ª–æ—Å—Ç—å'] += 1
                        if random.randint(1, 20) in [1, 6]:
                            print("–í–∞–º –ø–æ–≤–µ–∑–ª–æ –∏ –≤—ã –Ω–∞—à–ª–∏ –µ—â—ë –∑–µ–ª—å–µ–∫")
                            inventory["–ó–µ–ª—å–µ –∑–¥–æ—Ä–æ–≤—å—è"] += random.randint(1, 2)
                    else:
                        HERO["3"] = HERO["3"] - ENEMY["Hand"] + inventory["–ë—Ä–æ–Ω—è"] + 10 + inventory['—â–∏—Ç']
                        print(f"‚öîÔ∏è{NAME} –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–æ–ª—Å—è –º–æ–º–µ–Ω—Ç–æ–º –∏ –Ω–∞–Ω–µ—Å —É—Ä–æ–Ω")
                        Hero_health_bar = int(HERO['3'] // 10) * health_icon
                        print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {name} \n{name}-{Hero_health_bar}')
                if ENEMY["Health"] < 50 and run != 1:
                    if ENEMY["–ó–µ–ª—å—è"] > 0:
                        ENEMY["Health"] += random.randint(1, 60)
                        Enemy_bar_health_bar = ENEMY['Health'] * Enemy_bar_health_icon
                        ENEMY["–ó–µ–ª—å—è"] -= 1
                        print(f'----->üß™ {NAME} –≤—ã–ª–µ—á–µ–ª—Å—è –∏ —Ç–µ–ø–µ—Ä—å —É –Ω–µ–≥–æ  \n{NAME} - {Enemy_bar_health_bar}')
                        if random.randint(1, 6) in [1, 2, 3, 6]:
                            print(f"‚öîÔ∏è{name} –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–æ–ª—Å—è –º–æ–º–µ–Ω—Ç–æ–º –∏ –Ω–∞–Ω–µ—Å —É—Ä–æ–Ω")
                            ENEMY["Health"] = ENEMY["Health"] - HERO["–ë–∞–∑–æ–≤—ã–π —É—Ä–æ–Ω"] - inventory['–º–µ—á']
                            Enemy_bar_health_bar = ENEMY['Health'] * Enemy_bar_health_icon
                            print(f'-----> –ó–¥–æ—Ä–æ–≤—å–µ {NAME} \n{NAME} - {Enemy_bar_health_bar}')
                    else:
                        pass
            if HERO["3"] < 0 and ENEMY["Health"] < 0:
                print("–ö–ê–ö –í–´ –û–ë–ê –£–ú–ï–†–õ–ò ? –ß—Ç–æ–∂ —Å—á–∏—Ç–∞–π—Ç–µ —á—Ç–æ —ç—Ç–æ –Ω–∏—á—å—è")
            if HERO["3"] < 0:
                smerti = 1
                Endwin = 2
                fight = 0
                s = random.randint(1, 4)
                if s == 1:
                    print(f"–¢—ã –ø–æ–º–µ—Ä, {name} –ø–æ–ø—Ä–æ–±—É–π –µ—â—ë —Ä–∞–∑")
                if s == 2:
                    print(f"–í —Ç–µ–±—è –∏ —Ç–∞–∫ –Ω–µ –∫—Ç–æ –Ω–µ –≤–µ—Ä–∏–ª, {name} –ø–æ–ø—Ä–æ–±—É–π –µ—â—ë —Ä–∞–∑")
                if s == 3:
                    print(f"–ù–µ —Å–¥–∞–≤–∞–π—Å—è!, {name} –ø–æ–ø—Ä–æ–±—É–π –µ—â—ë —Ä–∞–∑, –≤ —ç—Ç–æ—Ç —Ä–∞–∑ —Ç–µ–±–µ –±–æ–ª—å—à–µ –ø–æ–≤–µ–∑–µ—Ç")
                if s == 4:
                    print(f"–¢—ã... {name} –ø–æ–ø—Ä–æ–±—É–π –µ—â—ë —Ä–∞–∑")
                time.sleep(4)
                print('–ü–µ—Ä–µ—Ö–æ–¥–∏–º –Ω–∞ –≥–ª–∞–≤–Ω—ã–π —ç–∫—Ä–∞–Ω')
                time.sleep(4)
            if ENEMY["Health"] < 0:
                if run == 1:
                    if slosnot == 1:
                        ProtScet = random.randint(5, 8)
                    if slosnot == 2:
                        ProtScet = random.randint(4, 6)
                    if slosnot == 3:
                        ProtScet = random.randint(3, 5)
                    if slosnot == 4:
                        ProtScet = random.randint(2, 4)
                    fight = 0
                    mapgame = 1

                    time.sleep(4)
                    print("–í–æ–∑—Ä–∞—à–∞–µ–º—Å—è –Ω–∞ –∫–∞—Ä—Ç—É...")
                    time.sleep(3)
                    rg = 0
                else:
                    if run == 0:
                        rm = 0
                        if slosnot == 1:
                            rm = random.randint(30, 35)
                            inventory["Money"] += rm
                        if slosnot == 2:
                            rm = random.randint(20, 25)
                            inventory["Money"] += rm
                        if slosnot == 3:
                            rm = random.randint(15, 20)
                            inventory["Money"] += rm
                        if slosnot == 4:
                            rm = random.randint(10, 18)
                            inventory["Money"] += rm
                    print(f"{NAME} —É–º–µ—Ä, –ü–û–ë–ï–î–êüéâüéâüéâ")
                    if NAME !='–ë–æ—Å—Å':

                        print('–í—ã –ø–æ–ª—É—á–∏–ª–∏', rm, '–º–æ–Ω–µ—Ç')
                        if slosnot == 1:
                            ProtScet = random.randint(5, 8)
                        if slosnot == 2:
                            ProtScet = random.randint(4, 6)
                        if slosnot == 3:
                            ProtScet = random.randint(3, 5)
                        if slosnot == 4:
                            ProtScet = random.randint(2, 4)
                        fight = 0
                        mapgame = 1

                        time.sleep(4)
                        print("–í–æ–∑—Ä–∞—à–∞–µ–º—Å—è –Ω–∞ –∫–∞—Ä—Ç—É...")
                        time.sleep(3)
                        rg = 0
                    else:
                        print('–í—ã –ø–æ–ª—É—á–∏–ª–∏, —Å–≤–æ–±–æ–¥—É')
                        time.sleep(5)
                        win = 1
                        fight = 0
                        exit()
    if smerti == 1:
        loop = input("–¢—ã —É–º–µ—Ä... –¢—ã —ç—Ç–æ –∑–Ω–∞–µ—à? –¢–∞–∫ –≤–æ—Ç, —Ö–æ—á–µ—à—å –∑–∞–Ω–æ–≥–æ –Ω–∞—á–∞—Ç—å? \n (–î–ê) --- (–ù–ï–¢)").lower()
        win = 0
        while loop != "–î–ê" and loop != '–î–∞' and loop != '–¥–∞' and loop != '–Ω–µ—Ç' and loop != '–ù–ï–¢' and loop != '–ù–µ—Ç':
            loop = input("–¢—ã —É–º–µ—Ä... –¢—ã —ç—Ç–æ –∑–Ω–∞–µ—à? –¢–∞–∫ –≤–æ—Ç, —Ö–æ—á–µ—à—å –∑–∞–Ω–æ–≥–æ –Ω–∞—á–∞—Ç—å? \n (–î–ê) --- (–ù–ï–¢)").lower()
            win = 0
    if win == 1:
        loop = input("–¢—ã –ø–æ–±–µ–¥–∏–ª, –Ω–æ –≥–æ—Ç–æ–≤ —Ç—ã –Ω–∞—á–∞—Ç—å –µ—â—ë  —Ä–∞–∑? \n (–î–ê) --- (–ù–ï–¢)").lower()
        while loop != "–î–ê" and loop != '–î–∞' and loop != '–¥–∞' and loop != '–Ω–µ—Ç' and loop != '–ù–ï–¢' and loop != '–ù–µ—Ç':
            loop = input("–¢—ã –ø–æ–±–µ–¥–∏–ª, –Ω–æ –≥–æ—Ç–æ–≤ —Ç—ã –Ω–∞—á–∞—Ç—å –µ—â—ë  —Ä–∞–∑? \n (–î–ê) --- (–ù–ï–¢)").lower()
    if loop == '–¥–∞':
        loop = 1
        Endwin = 0
        mapgame = 0
        smerti = 0
        win = 0
        rg = 0
        magaz = 1
        NAME = '–ü—Ä–æ—Ç–∏–≤–Ω–∏–∫'
    if loop == '–Ω–µ—Ç':
        loop = 0
        mapgame = 0
        smerti = 0
        win = 0
        Endwin = 0
